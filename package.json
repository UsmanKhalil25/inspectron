{
  "name": "inspectron",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "ui",
    "core",
    "api",
    "shared"
  ],
  "scripts": {
    "build:all": "npm run build --workspace=shared && npm run build --workspace=core && npm run build --workspace=api",
    "lint": "npm run lint --workspaces",
    "format": "npm run format --workspaces",
    "format:check": "npm run format:check --workspaces",
    "install:all": "npm install"
  }
}
